@using SergeiLevin0.ViewModels 
@model IEnumerable<SergeiLevin0.ViewModels.EmployeeView>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetEmployees</title>
</head>
<body>
<h3>@ViewBag.SomeData</h3>
<h4>@ViewData["Test"]</h4>
<table>
    <thead>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>SecondName</th>
        <th>Age</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var employee in Model)
    {
        <tr>
            <td>@employee.Id</td>
            <td>@employee.FirstName</td>
            <td>@employee.SecondName</td>
            <td>@employee.Age</td>
        </tr>
    }
    </tbody>

</table>

</body>
</html>
